{
  "name": "adp",
  "version": "1.0.0",
  "description": "adp",
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build:test": "vue-cli-service build --mode test",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "build:byd": "vue-cli-service build --env OBJECT_TYPE=BYD",
    "preview": "node build/index.js --preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "lint": "eslint --ext .js,.vue src",
    "test": "jest",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "report": "set npm_config_report=true && vue-cli-service build",
    "prepare": "husky install",
    "lint-staged": "lint-staged --allow-empty"
  },
  "dependencies": {
    "@antv/g6": "^3.4.4",
    "@antv/x6": "^2.16.1",
    "@antv/x6-plugin-keyboard": "^2.2.1",
    "@antv/x6-plugin-selection": "^2.2.1",
    "@riophae/vue-treeselect": "^0.4.0",
    "@xunlei/vue-context-menu": "^1.0.2",
    "airongkeji-rule-editor": "^3.1.90",
    "autofit.js": "^2.0.1",
    "axios": "0.18.1",
    "circular-json": "^0.5.9",
    "core-js": "3.6.5",
    "echarts": "^4.9.0",
    "element-ui": "2.13.2",
    "insert-css": "^2.0.0",
    "js-cookie": "2.2.0",
    "jsencrypt": "^3.3.2",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "uuidv7": "^1.0.2",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.11",
    "vue-fragment": "^1.6.0",
    "vue-i18n": "^8.23.0",
    "vue-json-viewer": "^2.2.22",
    "vue-router": "3.0.6",
    "vue2-ace-editor": "0.0.15",
    "vuedraggable": "^2.24.3",
    "vuex": "3.1.0",
    "vxe-table": "^3.12.9",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "4.4.4",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "9.5.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "23.6.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chalk": "2.4.2",
    "compression-webpack-plugin": "^6.1.1",
    "connect": "3.6.6",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "^5.0.0-beta.1",
    "husky": "^8.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^13.0.3",
    "mockjs": "1.0.1-beta3",
    "node-sass": "^4.11.0",
